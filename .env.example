# Solana Configuration
# Get RPC URL from: https://www.quicknode.com/ or https://www.helius.dev/
SOLANA_RPC_URL=https://api.mainnet-beta.solana.com

# Solana private key for Agent Kit (base58 encoded)
# WARNING: Keep this secure! Never commit to version control
SOLANA_PRIVATE_KEY=your_base58_private_key_here

# PostgreSQL Connection
# Format: postgresql://username:password@localhost:5432/database_name
POSTGRES_CONNECTION_STRING=postgresql://user:password@localhost:5432/dbname

# Sentry Configuration
# Get from: https://sentry.io/settings/account/api/auth-tokens/
SENTRY_AUTH_TOKEN=your_sentry_auth_token_here
SENTRY_ORG_SLUG=your_organization_slug

# Zapier Configuration
# Get from: https://zapier.com/app/developer
ZAPIER_API_KEY=your_zapier_api_key_here

# Apidog Configuration
# Get from: https://apidog.com/
APIDOG_API_KEY=your_apidog_api_key_here
